{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "CSF.Extensions.WebDriver.Tests/**",
            "docs/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": []
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../docs",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "CSF.Extensions.WebDriver",
      "_appTitle": "CSF.Extensions.WebDriver",
      "_enableSearch": true,
      "pdf": false
    }
  }
}